{
    "name": "aon4o/whmcs-helpers",
    "description": "A collection of WHMCS specific helpers.",
    "keywords": [
        "aon4o",
        "whmcs-helpers",
        "whmcs",
        "helpers",
        "whmcs-module"
    ],
    "homepage": "https://github.com/aon4o/whmcs-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Naida",
            "email": "a.o.naidenov@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-imap": "*",
        "psy/psysh": "^0.12.10"
    },
    "require-dev": {
        "laravel/pint": "^1.20",
        "pestphp/pest": "^2.36",
        "whmcs/developer-docs": "dev-main",
        "abraham/twitteroauth": "^2.0",
        "composer/composer": "^2.0",
        "ezyang/htmlpurifier": "^4.12.0",
        "filp/whoops": "^2.7.2",
        "firebase/php-jwt": "^5.2.0",
        "google/apiclient": "^2.7.0",
        "google/auth": "^1.11.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/console": "^9.0",
        "illuminate/container": "^9.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/events": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/validation": "^9.0",
        "knplabs/knp-menu": "^3.1.1",
        "laminas/laminas-diactoros": "^2.3.1",
        "laminas/laminas-httphandlerrunner": "^1.2.0",
        "league/climate": "^3.5.2",
        "league/flysystem": "^1.0.6",
        "league/oauth2-client": "^2.5.0",
        "league/oauth2-google": "^3.0.3",
        "monolog/monolog": "^2.0.2",
        "nikic/fast-route": "^1.3.0",
        "php-imap/php-imap": "^3.1.0",
        "phpmyadmin/sql-parser": "^5.3.1",
        "phpseclib/mcrypt_compat": "^1.0.1",
        "phpseclib/phpseclib": "^2.0.2",
        "punic/punic": "^3.5.1",
        "ramsey/uuid": "^4.0.1",
        "react/promise": "^2.8.0",
        "seld/jsonlint": "^1.8.0",
        "seld/phar-utils": "^1.1.0",
        "stripe/stripe-php": "^7.34.0",
        "symfony/polyfill-ctype": "^1.18.1",
        "symfony/polyfill-iconv": "^1.18.1",
        "symfony/polyfill-intl-idn": "^1.18.1",
        "symfony/polyfill-mbstring": "^1.18.1",
        "symfony/polyfill-php72": "^1.18.1",
        "symfony/polyfill-php73": "^1.18.1",
        "tecnickcom/tcpdf": "^6.3.5",
        "zbateson/stream-decorators": "^1.0.3",
        "smarty/smarty": "^3.1.36"
    },
    "autoload": {
        "psr-4": {
            "Aon4o\\WhmcsHelpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "bin": [
        "bin/whmcs-shell"
    ],
    "scripts": {
        "test": "vendor/bin/pest",
        "test:unit": "@test --testsuite=Unit",
        "test:feat": "@test --testsuite=Feature",
        "test:arch": "@test --testsuite=Architecture",
        "lint": "vendor/bin/pint",
        "lint:test": "@lint --test",
        "format": "vendor/bin/rector",
        "format:test": "@format --dry-run"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "whmcs/developer-docs",
                "version": "dev-main",
                "source": {
                    "type": "git",
                    "url": "https://github.com/WHMCS/developer-docs.git",
                    "reference": "master"
                }
            }
        }
    ]
}
